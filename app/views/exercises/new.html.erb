<% provide(:title, 'New Exercise') %>
<h1>Create new exercise</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@exercise) do |f| %>
			<%= render 'shared/error_messages' %>
			<%=f.label :name %>
			<%=f.text_field :name %>

			<%= f.label :description %>
			<%= f.text_field :description%>

			<%= f.submit "Create new exercise", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
